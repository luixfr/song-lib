{
  "titles": ["Libro Bendito"],
  "authors": ["Autor desconocido"],
  "languages": ["spanish"],
  "songbooks": [{ "name": "Nuestos Himnos Predilectos", "number": "91" }],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "c1", "v2", "c1", "v3", "c1", "v4"],
      "lyric": {
        "v1": [
          "Libro bendito, tu bella luz,",
          "que a los mortales guía a Jesús;",
          "es de esperanza claro fanal,",
          "¡Libro bendito, inmortal!"
        ],
        "c1": [
          "¡Brilla!, ¡Brilla!",
          "Faro de eternos fulgores",
          "¡Brilla!, ¡Brilla!",
          "Disipa nuestros errores.",
          "¡Brilla!, ¡Brilla!",
          "Guía a los pecadores.",
          "Libro inmortal ",
          "don sin igual",
          "nunca tu luz faltará."
        ],
        "v2": [
          "Libro admirable, Dios es tu autor.",
          "Siempre en las lides del vencedor,",
          "fuerte nos haces tu contra el mal.",
          "¡Libro admirable, inmortal!"
        ],
        "v3": [
          "Libro divino, revelación",
          "del plan eterno de salvación.",
          "incomparable don celestial,",
          "¡Libro divino, inmortal!"
        ],
        "v4": [
          "Preciosa historia del buen Jesús,",
          "en ti los sabios buscan la luz",
          "paz y esperanza, fuerza y valor,",
          "das al cristiano viador."
        ]
      }
    }
  ],
  "versions": []
}
