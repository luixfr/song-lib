{
  "titles": ["POR FE CONTEMPLO REDENCIÓN"],
  "authors": ["Autor desconocido"],
  "languages": ["spanish"],
  "songbooks": [{ "name": "Nuestos Himnos Predilectos", "number": "156" }],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "c1", "v2", "c1", "v3", "c1", "v4", "c1"],
      "v1": [
        "Por fe contemplo redención :",
        "La fuente carmesí.",
        "Jesús nos da la salvación,",
        "su vida dio por mí."
      ],
      "c1": [
        "La fuente sin igual hallé:",
        "De vida y luz el manantial;",
        "¡Oh, gloria a Dios, me limpia a mí,",
        "me limpia a mí, me limpia a mí!"
      ],
      "v2": [
        "Mi vida entrego a mi Jesús,",
        "las dudas El quitó;",
        "mi alma goza de su luz,",
        "mis deudas El pagó."
      ],
      "v3": [
        "¡Cuán inefable gozo es,",
        "saber que salvo soy!",
        "Entronizado está,",
        "en este corazón."
      ],
      "v4": [
        "¡Oh, gracia excelsa de mi Dios,",
        "profundo es el amor!,",
        "De mi Jesús, mi buen Jesús,",
        "Cordero Redentor."
      ]
    }
  ],
  "versions": []
}
