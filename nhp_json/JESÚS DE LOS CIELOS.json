{
  "titles": ["JESÚS DE LOS CIELOS"],
  "authors": ["Autor desconocido"],
  "languages": ["spanish"],
  "songbooks": [{ "name": "Nuestos Himnos Predilectos", "number": "299" }],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "c1", "v2", "c1", "v3", "c1", "v4", "c1", "v5", "c1"],
      "v1": [
        "Jesús de los cielos,",
        "al mundo bajó,",
        "en busca de joyas",
        "que amante compró."
      ],
      "c1": [
        "Los niños salvados ",
        "serán como el sol, ",
        "brillando en la gloria ",
        "del Rey Salvador."
      ],
      "v2": [
        "Angustias y muerte",
        "y horrible aflicción,",
        "costaron las joyas",
        "que amante compró."
      ],
      "v3": [
        "Su hermosa diadema,",
        "de eterno esplendor ,",
        "la adornan las joyas",
        "que amante compró."
      ],
      "v4": [
        "Los niños y niñas",
        "que van al Señor ,",
        "son todos, las joyas",
        "que amante compró."
      ],
      "v5": [
        "Venid, pues, alegres,",
        "al buen Redentor;",
        "Él quiere las joyas",
        "que amante compró."
      ]
    }
  ],
  "versions": []
}
