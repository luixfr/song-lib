{
  "titles": ["ES CRISTO LA PASIÓN"],
  "autors": ["Autor desconocido"],
  "languages": ["spanish"],
  "songbooks": [{ "name": "Nuestos Himnos Predilectos", "number": "276" }],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "c1", "v2", "c1"],
      "v1": [
        "Es Cristo la pasión del “MIEPI”",
        "guiado de Poder Divino.",
        "Dio la inspiración la gloria de Dios",
        "y hoy resuena su existencia."
      ],
      "c1": [
        "Marchando siempre el “MIEPI” alegre,",
        "nos da su brillante historia;",
        "mil años que fue de lucha tenaz",
        "y ha vencido con valor."
      ],
      "v2": [
        "Sigamos en su ejemplo unidos,",
        "colmados de bendiciones;",
        "hasta encontrar la gloria final,",
        "que será el de nuestro triunfo."
      ]
    }
  ],
  "versions": []
}
