{
  "titles": ["Siete Palabras Son"],
  "authors": ["Autor desconocido"],
  "languages": ["spanish"],
  "songbooks": [{ "name": "Nuestos Himnos Predilectos", "number": "469" }],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "c1", "v2", "c1", "v3", "c1", "v4", "c1", "v5"],
      "lyric": {
        "v1": [
          "Siete palabras son, ",
          "que hablo cristo Jesús",
          "estando en la cruz;",
          "nos muestras perfección."
        ],
        "c1": [
          "Bendita redención",
          "nos muestra el salvador,",
          "Siete palabras son.",
          "¡Sublime redención!."
        ],
        "v2": [
          "Enseña el perdón",
          "que hay que practicar.",
          "Paraíso el prometió,",
          "el cual nos ha de dar."
        ],
        "v3": [
          "Confirma el amor",
          "que no se violara;",
          "si vivimos así,",
          "Él nos ampara."
        ],
        "v4": [
          "Saciemos al señor,",
          "como el hace a nos,",
          "El mal devorador",
          "Jesús lo consumió."
        ],
        "v5": [
          "Solo hay que encomendar",
          "nuestra alma a mi Jesús;",
          "y así a la gloria entrar,",
          "teniendo eterna luz."
        ]
      }
    }
  ],
  "versions": []
}
