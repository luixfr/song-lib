{
  "titles": ["Aunque Soy Pequeñuelo"],
  "authors": ["Autor desconocido"],
  "languages": ["spanish"],
  "songbooks": [{ "name": "Nuestos Himnos Predilectos", "number": "295" }],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "v2", "v3"],
      "lyric": {
        "v1": [
          "Aunque soy pequeñuelo,",
          "me mira el Santo Dios;",
          "Él oye desde el Cielo",
          "mi humilde y tierna voz."
        ],
        "v2": [
          "Me ve de su Alto Asiento,",
          "mi nombre sabe, sí;",
          "y cuanto pienso y siento",
          "conoce desde allí."
        ],
        "v3": [
          "Él mira a cada instante",
          "lo que hago, bien o mal,",
          "pues todo está delante,",
          "de su Ojo Paternal."
        ]
      }
    }
  ],
  "versions": []
}
