{
  "titles": ["ID A LOS GENTILES"],
  "autors": ["Autor desconocido"],
  "languages": ["spanish"],
  "songbooks": [{ "name": "Nuestos Himnos Predilectos", "number": "69" }],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "v2", "v3", "v4", "v5", "v6"],
      "v1": [
        "Id a los Gentiles,",
        "doctrinad a todos;",
        "ya los que creyeren,",
        "los bautizareis.",
        "El Dueño y Rey de nuestras almas",
        "así lo ordenó."
      ],
      "v2": [
        "En Nombre del Padre,",
        "en Nombre del Hijo",
        "y del Santo Espíritu,",
        "en Nombre también;",
        "es la enseñanza sacrosanta",
        "de la invocación."
      ],
      "v3": [
        "Doctrina bendita,",
        "que hemos recibido;",
        "pues el que creyere",
        "y fuere sumergido:",
        "Al Libro eterno de la Vida",
        "su nombre entrará."
      ],
      "v4": [
        "Las nuevas criaturas",
        "fueron sumergidas,",
        "los nuevos cristianos",
        "fueron bautizados,",
        "en agua fueron sepultados",
        "los hijos de Dios."
      ],
      "v5": [
        "Pero resurgieron",
        "los que obedecieron,",
        "al mundo murieron,",
        "a la iglesia entrarán.",
        "y así quedó complementada",
        "su gran salvación."
      ],
      "v6": [
        "La Iglesia de Cristo",
        "recibe Ordenanza:",
        "Bautismo primero,",
        "la Cena después;",
        "y el que practique la doctrina,",
        "Cristiano ya es."
      ]
    }
  ],
  "versions": []
}
