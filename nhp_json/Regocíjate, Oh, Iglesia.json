{
  "titles": ["Regocíjate, Oh, Iglesia"],
  "authors": ["Autor desconocido"],
  "languages": ["spanish"],
  "songbooks": [{ "name": "Nuestos Himnos Predilectos", "number": "474" }],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "c1", "v2", "c1", "v3", "c1", "v4"],
      "lyric": {
        "v1": [
          "Regocíjate, oh Iglesia fiel,",
          "y con júbilo cantad.",
          "Ya resucito tu salvador",
          "de la tumba, es la verdad."
        ],
        "c1": [
          "Hermosa verdad: ",
          "Él vive ya, entre su grey.",
          "Cristo vive ya ",
          "y muy pronto volverá."
        ],
        "v2": [
          "Y la muerte ya el destruyo,",
          "su imperio aplasto.",
          "Del sepulcro ya se levanto",
          "y victoria así nos dio."
        ],
        "v3": [
          "Ya no temas tú, pequeña grey,",
          "que al sepulcro triste estas.",
          "¡Cristo vive ya! Se levanto",
          "Tu tristeza Él llevó."
        ],
        "v4": [
          "Regocíjate, oh, Iglesia fiel,",
          "que Jesús resucito",
          "Y nos resucita con poder.",
          "¡Aleluya! ¡Gloria! ¡Amen!"
        ]
      }
    }
  ],
  "versions": []
}
