{
  "titles": ["Santo, Dios Poderoso"],
  "authors": ["Autor desconocido"],
  "languages": ["spanish"],
  "songbooks": [{ "name": "Nuestos Himnos Predilectos", "number": "36" }],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "v2", "v3"],
      "lyric": {
        "v1": [
          "/: Santo, Dios poderoso,",
          "Príncipe hermoso,",
          "quiero estar cerca de Ti:/",
          "Llenos están los cielos de tu grandeza, ¡Oh, Dios!",
          "Jehová, Tú eres glorioso, digno de todo honor."
        ],
        "v2": [
          "/: Santo, yo quiero ser,",
          "Tú lo has mandado;",
          "hazme santo, mi Señor:/",
          "Lléname más de Ti, quiero sentirte en mí;",
          "para poder mirarte, dame mas santidad."
        ],
        "v3": [
          "Santo, ¿quién como Tú?",
          "Limpiaste mi maldad.",
          "¡Gloria a Ti, gloria Ti!",
          "Santo, Dios poderoso, Príncipe hermoso, quiero estar cerca de Ti."
        ]
      }
    }
  ],
  "versions": []
}
