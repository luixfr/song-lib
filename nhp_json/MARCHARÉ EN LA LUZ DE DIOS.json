{
  "titles": ["MARCHARÉ EN LA LUZ DE DIOS"],
  "authors": ["Autor desconocido"],
  "languages": ["spanish"],
  "songbooks": [{ "name": "Nuestos Himnos Predilectos", "number": "321" }],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "c1", "v2", "c1", "v3", "c1", "v4", "c1"],
      "v1": [
        "Marcharé en la luz de Dios, ",
        "marcharé, siempre marcharé. ",
        "Por la senda que Jesús trazó, ",
        "siempre marcharé."
      ],
      "c1": [
        "Vestido blanco, ama y luz, ",
        "corona áurea, un bello hogar; ",
        "y laurel de triunfo, gozo y paz ",
        "tendré yo al llegar."
      ],
      "v2": [
        "Marcharé con valor y fe ",
        "marcharé, siempre marcharé; ",
        "la victoria en Jesús tendré, ",
        "siempre marcharé."
      ],
      "v3": [
        "Marcharé con valor y fe, ",
        "Marcharé, siempre marcharé.",
        "En su Espíritu, sin temor",
        "Siempre marchare."
      ],
      "v4": [
        "Marchen, marchen en pos de Dios, ",
        "abandonen la vida atroz; ",
        "obedezcan al Justo Juez; ",
        "marchen, marchen, pues."
      ]
    }
  ],
  "versions": []
}
