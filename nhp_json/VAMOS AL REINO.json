{
  "titles": ["VAMOS AL REINO"],
  "autors": ["Autor desconocido"],
  "languages": ["spanish"],
  "songbooks": [{ "name": "Nuestos Himnos Predilectos", "number": "98" }],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "c1", "v2", "c1", "v3", "c1", "v4", "c1"],
      "v1": [
        "Vamos al Reino del Señor,",
        "todos cantando con amor.",
        "A la Ciudad Jerusalem,",
        "que es nuestra Patria Celestial."
      ],
      "c1": [
        "La Santa Biblia",
        "es nuestra Guía.",
        "Es la Palabra de Jehová,",
        "ya la victoria nos llevará."
      ],
      "v2": [
        "Cuando te sientas desmayar",
        "clama: ¡La Sangre de Jesús!",
        "y nuevas fuerzas tú tendrás,",
        "y la batalla ganarás."
      ],
      "v3": [
        "No temas ir contra del mal,",
        "porque en tu diestra llevarás,",
        "la Espada que tú empuñarás,",
        "y la Bandera que es Jehová."
      ],
      "v4": [
        "Pero tenemos que luchar",
        "contra el maligno tentador",
        "pero tenemos del Señor",
        "la ayuda del Consolador."
      ]
    }
  ],
  "versions": []
}
