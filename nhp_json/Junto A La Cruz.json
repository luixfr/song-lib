{
  "titles": ["Junto A La Cruz"],
  "authors": ["Autor desconocido"],
  "languages": ["spanish"],
  "songbooks": [{ "name": "Nuestos Himnos Predilectos", "number": "25" }],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "c1", "v2", "c1", "v2", "c1", "v3"],
      "lyric": {
        "v1": [
          "Junto a la cruz, do Jesús murió",
          "Junto a la cruz, do salud pedí",
          "Ya mis maldades el perdono",
          "/:a su Nombre gloria:/"
        ],
        "c1": [
          "/: ¡A su Nombre gloria:/",
          "Ya mis maldades el perdono",
          "¡A su Nombre gloria!"
        ],
        "v2": [
          "Fuente preciosa de salvación",
          "Que grande gozo yo pude hallar",
          "Al encontrar en Jesús perdón",
          "A su Nombre gloria"
        ],
        "v3": [
          "Tu pecador que perdido estas",
          "Hoy esta fuente ven a buscar",
          "Paz y perdón encontrar podrás",
          "A su Nombre gloria."
        ]
      }
    }
  ],
  "versions": []
