{
  "titles": ["A CRISTO DOY MI CANTO"],
  "authors": ["Autor desconocido"],
  "languages": ["spanish"],
  "songbooks": [{ "name": "Nuestos Himnos Predilectos", "number": "1" }],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "c1", "v2", "c1", "v3", "c1", "v4", "c1"],
      "v1": [
        "A Cristo doy mi canto,",
        "El salva el alma mía;",
        "me libra del quebranto",
        "y con amor me guía."
      ],
      "c1": [
        "Ensalce nuestro canto",
        "tu sacrosanta historia.",
        "Es nuestro anhelo santo",
        "mirar, Jesús, tu gloria."
      ],
      "v2": [
        "Jamás, dolor ni agravios",
        "enlutarán la mente,",
        "si a Cristo, nuestros labios",
        "bendicen dulcemente."
      ],
      "v3": [
        "Tu Nombre bendecido",
        "alegra el alma mía;",
        "tu Nombre es en mi oído",
        "dulcísima armonía."
      ],
      "v4": [
        "Viviendo he de ensalzarte,",
        "y si abandono el suelo,",
        "veránme ir a adorarte",
        "los ángeles del cielo."
      ]
    }
  ],
  "versions": []
}
