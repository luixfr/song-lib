{
  "titles": ["TE LOAMOS, ¡OH DIOS!"],
  "authors": ["Autor desconocido"],
  "languages": ["spanish"],
  "songbooks": [{ "name": "Nuestos Himnos Predilectos", "number": "39" }],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "c1", "v2", "c1", "v3", "c1", "v4", "c1"],
      "v1": [
        "Te loamos, ¡oh, Dios!",
        "Con unánime voz,",
        "que en Cristo, tu Hijo,",
        "nos diste perdón."
      ],
      "c1": [
        "¡Aleluya! Te alabamos.",
        "¡Cuán grande es tu amor!",
        "¡Aleluya! Te adoramos,",
        "bendito Señor."
      ],
      "v2": [
        "Te loamos, Jesús,",
        "quien tu trono de luz",
        "has dejado, por darnos",
        "salud en la cruz."
      ],
      "v3": [
        "Te damos loor,",
        "por el Consolador ,",
        "que nos llena de gozo",
        "y santo valor."
      ],
      "v4": [
        "Unidos load",
        "a la gran Trinidad,",
        "que es la Fuente de Gracia,",
        "Virtud y Verdad."
      ]
    }
  ],
  "versions": []
}
