{
  "titles": ["Vamos A Alabar Su Nombre"],
  "authors": ["Autor Desconocido"],
  "songbooks": [{ "name": "Himnario de coros", "number": "474" }],
  "languages": ["spanish"],
  "versions": [],
  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "v2", "v3", "v4"],
      "lyric": {
        "v1": [
          "/: Vamos alabar a su Nombre,",
          "vamos a glorificarle:/",
          "/: Porque Cristo vive,"
        ],
        "v2": [
          "porque Cristo reina",
          "porque Cristo vive se mueve,",
          "se mueva en mi corazón:/"
        ],
        "v3": [
          "/: En la cárcel Pablo",
          "y Silas alababan al Señor:/",
          "/: Y la gloria de Dios bajó y la gloria se derramó/"
        ],
        "v4": [
          "/: La Iglesia primitiva estaba en oración:/",
          "/: Y vino el Espíritu Santo y con Fuego se derramó, llenándolos con su gloria y",
          "llenándolos con su Poder:/"
        ]
      }
    }
  ]
}
