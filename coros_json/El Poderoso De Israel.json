      "lyric": {
        "v1": [
          "Él es el Poderoso de Israel",
          "su voz se oirá nadie lo detendrá",
          "al Poderoso de Israel"
        ],
        "v2": ["-1-", "Y de noche cantaremos", "celebrando su poder"],
        "v3": [
          "con alegría de corazón",
          "como el que va con la flauta",
          "al monte de Jehová"
        ],
        "v4": ["celebraremos su Poder.", "-2-", "Y los ojos de los ciegos"],
        "v5": [
          "se abrirán y nos verán",
          "los oídos de los sordos oirán",
          "el cojo saltará, con el arpa alabará"
        ],
        "v6": ["la lengua de los mudos cantará."]
      }
    }
  ]
