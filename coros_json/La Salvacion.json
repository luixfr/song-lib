      "lyric": {
        "v1": [
          "La salvación es de nuestro Dios,",
          "El está sentado sobre su trono.",
          "Y al Cordero de Dios toda la gloria,"
        ],
        "v2": ["sabiduría, honor y todo el poder.", "Coro:", "A nuestro Dios,"],
        "v3": [
          "ahora y siempre, Amén...",
          "Y los redimidos cantarán",
          "a una voz y unidad."
        ],
        "v4": [
          "Y le declararán toda la gloria,",
          "sabiduría, honor y todo el poder."
        ]
      }
    }
  ]
