  "lyrics": [
    {
      "language": "spanish",
      "order": ["v1", "v2", "v3", "v4"],
      "lyric": {
        "v1": [
          "Dios es nuestro amparo,",
          "nuestra fortaleza",
          "nuestro pronto auxilio en la tribulación"
        ],
        "v2": [
          "Aunque se traspasen",
          "los montes a la mar",
          "aunque la tierra tiemble"
        ],
        "v3": [
          "tenemos que cantar",
          "/: Cielo y tierra pasarán",
          "más su Palabra no Pasará:/"
        ],
        "v4": ["no, no, no pasará"]
      }
    }
  ]
}
