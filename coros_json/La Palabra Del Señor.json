      "lyric": {
        "v1": [
          "La Palabra del Señor es fiel,",
          "sus promesas cumplirá",
          "cielo y tierra pasarán"
        ],
        "v2": [
          "más su Palabra no pasará.",
          "El ha dicho que al Cielo irá",
          "todo aquel que crea en El;"
        ],
        "v3": [
          "la Palabra del Señor es fiel",
          "sus promesas cumplirá",
          "en todo aquel que crea en El."
        ]
      }
