      "language": "spanish",
      "order": ["v1", "v2", "v3", "v4", "v5", "v6", "v7"],
      "lyric": {
        "v1": [
          "Juventud es belleza",
          "juventud es encanto",
          "juventud es perfume"
        ],
        "v2": [
          "si se rinde al Señor.",
          "Pasan, pasan los años",
          "una flor se marchita"
        ],
        "v3": ["pero en Cristo bendito", "sólo habrá juventud.", "Coro:"],
        "v4": [
          "Del Señor acuérdate",
          "antes que lleguen",
          "los días malos diciendo:"
        ],
        "v5": [
          "'Contento no estoy'. < /p>El Maestro te llama",
          "su voz escucha atenta;",
          "adorna sus altares"
        ],
        "v6": [
          "dale tu juventud.",
          "En la cruz del calvario",
          "una flor traspasada"
        ],
        "v7": ["una flor ensangrentada", "juventud de Jesús."]
      }
    }
  ]
