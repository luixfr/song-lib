        "v1": [
          "Cantad alegres al Señor,",
          "toda la tierra alabad",
          "/: El, es mi Dios y mi Señor:/"
        ],
        "v2": [
          "/: Levantemos nuestra voz y aplaudid:/",
          "/: Levantemos nuestras manos",
          "y alabemos al Señor:/"
        ],
        "v3": [
          "Levantemos nuestra voz,",
          "¡Gloria a Dios! y aplaudid.",
          "/: Levantemos manos santas"
        ],
        "v4": ["y alabemos al Señor:/"]
      }
    }
  ]
