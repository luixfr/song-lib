        "v2": [
          "Se levanta el sol, tan cierto",
          "Como que le canto",
          "Y me puede escuchar."
        ],
        "v3": ["/: Le alabo: /", "y mi alma no se cansa", "de alabarle: /"],
        "v4": ["/: Le alabo:/", "Mientras viva", "y exista en mí ser."],
        "v5": [
          "/: Le adoro:/ y mi alma",
          "no se cansa de adorarle",
          "/: Le adoro:/"
        ],
        "v6": ["mientras viva y exista en mi ser."]
      }
    }
  ]
